{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "c1sar-assistant-queue-prd",
  "main": "src/queue/index.ts",
  "compatibility_date": "2025-11-15",
  "kv_namespaces": [
    {
      "binding": "COMMITS_REPORTS",
      "id": "aec90e3958f34f5f84c866494ce9ab61",
      "preview_id": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx3"
    },
    {
      "binding": "BOT_REPORTS",
      "id": "953282d1072a41ddac24b8d67dfb38f7",
      "preview_id": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx4"
    }
  ],
  "queues": {
    "producers": [
      {
        "queue": "reports-queue",
        "binding": "REPORTS_QUEUE"
      }
    ],
    "consumers": [
      {
        "queue": "reports-queue",
        "max_batch_size": 1,
        "max_batch_timeout": 30
      }
    ]
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  }
}

