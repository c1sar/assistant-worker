{
  "name": "assistant-worker",
  "type": "module",
  "scripts": {
    "dev:api": "wrangler dev --config wrangler.api.jsonc --persist-to .wrangler/state",
    "dev:queue": "wrangler dev --config wrangler.queue.jsonc --persist-to .wrangler/state",
    "deploy:api": "wrangler deploy --config wrangler.api.jsonc --minify",
    "deploy:queue": "wrangler deploy --config wrangler.queue.jsonc --minify",
    "deploy": "pnpm run deploy:api && pnpm run deploy:queue",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "hono": "^4.10.6"
  },
  "devDependencies": {
    "wrangler": "^4.4.0"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
